<h3 class="text-lg font-semibold text-gray-900 mb-3 mt-4">Liste des participants</h3>
<div class="flex flex-wrap gap-2">
  <% @session.registrations.each do |registration| %>
    <div class="flex items-center gap-2 px-2 py-1 bg-gray-50 rounded">
      <%= registration.user.full_name %>
    </div>
  <% end %>
</div>
